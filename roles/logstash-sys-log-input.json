  "default_attributes": {
    "logstash": {                                                                                                                                                                   
      "inputs": {
        "sys-log": {
          "port": 5610,
          "query": {
            "role": "logstash_elasticsearch_master",
            "tags": "sys-log",
            "chef_environment": "prod"
          }
        }
      }
    }
  },
  "override_attributes": {
  },
  "chef_type": "role",
